<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF"
    tools:context=".Fragments.Lista_Col_Salida_Fragment">

    <TextView
        android:id="@+id/text_gallery"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#D30606"
        android:text="Lista de Colegios"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/blanco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlCompat" />
    <TextView
        android:id="@+id/linelisbolalum"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/blanco"
        app:layout_constraintTop_toBottomOf="@id/text_gallery"
        android:layout_marginTop="5dp"/>
    <ListView
        android:id="@+id/lvListaCol"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/linelisbolalum"/>

</androidx.constraintlayout.widget.ConstraintLayout>
